// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ShortLink {
  id        String   @id @unique @default(cuid())
  url       String
  slug      String   @unique
  maxVisit  Int      
  createdAt DateTime @default(now())
}
